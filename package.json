{
  "name": "sanity-plugin-nextjs-do-deploy",
  "version": "0.3.0",
  "description": "Launch deployments from Sanity and NextJS",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/EricWVGG/sanity-plugin-nextjs-do-deploy#readme",
  "bugs": {
    "url": "https://github.com/EricWVGG/sanity-plugin-nextjs-do-deploy/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/EricWVGG/sanity-plugin-nextjs-do-deploy.git"
  },
  "license": "MIT",
  "author": "Eric Jacobsen <sanity@wvgg.co>",
  "scripts": {
    "build": "tsup",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "publish": "npm login && npm publish",
    "watch": "pkg-utils watch --strict"
  },
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    "./sanity": {
      "source": "./src/sanity/index.ts",
      "import": "./dist/sanity/index.mjs",
      "default": "./dist/sanity/index.js"
    },
    "./next": {
      "source": "./src/next/index.ts",
      "import": "./dist/next/index.mjs",
      "default": "./dist/next/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/sanity/index.js",
  "types": "./dist/sanity/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "sanityPlugin": {
    "verifyPackage": {
      "srcIndex": false,
      "nodeEngine": false
    }
  },
  "dependencies": {
    "@sanity/incompatible-plugin": "^1.0.5",
    "@sanity/ui": "^3.1.10",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "usehooks-ts": "^3.1.1"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "8.1.12",
    "@sanity/plugin-kit": "^4.0.20",
    "@types/react": "^19.2.6",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-sanity": "^7.1.4",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "sanity": "^4.18.0",
    "styled-components": "^6.1.19",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "yalc": "1.0.0-pre.53"
  },
  "peerDependencies": {
    "next": "^15.0.0",
    "react": "^18 || ^19",
    "react-dom": "^19.2.0",
    "sanity": "^4.14.2 || ^15.0.0"
  },
  "engines": {
    "node": ">=20"
  }
}
